{
  "name": "unocss-preset-tapp",
  "version": "1.0.0",
  "description": "Derives the unocss default preset but adds some animation rules and maps colors to css variables",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format=cjs,esm --dts",
    "lint": "eslint --flag unstable_ts_config src",
    "test": "echo 'linting is only form of testing currently'"
  },
  "keywords": [
    "unocss",
    "preset",
    "tapp"
  ],
  "author": "Ken Snyder <ken@ken.net>",
  "license": "ISC",
  "dependencies": {
    "@unocss/preset-uno": "0.65.0",
    "inferred-types": "^0.52.9",
    "unocss": "0.65.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@type-challenges/utils": "^0.1.1",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^0.1.3",
    "jiti": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
